{
  "name": "@devfunlabs/web-sdk",
  "version": "0.5.6",
  "description": "DevFun web SDK",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./styles.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css"
  ],
  "scripts": {
    "build": "ts-bridge build --project ./tsconfig.json --clean",
    "clean": "rm -rf dist",
    "dev": "nodemon --watch src --ext ts,tsx --exec 'npm run build'",
    "lint": "eslint \"src/**/*.ts\" --fix --max-warnings 0",
    "release": "pnpm build && npm publish --access public"
  },
  "dependencies": {
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.38",
    "@solana/web3.js": "^1.98.1",
    "axios": "^1.7.9",
    "bs58": "^6.0.0",
    "crypto-js": "^4.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsx": "^4.19.1",
    "zod": "3.22.3"
  },
  "sideEffects": [
    "styles.css"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/chai": "^4.3.20",
    "@types/crypto-js": "^4.2.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "chai": "^5.1.2",
    "mocha": "^10.5.2",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
